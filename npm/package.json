{
  "name": "@jxekav/go-fast",
  "version": "0.0.1",
  "description": "A fast JavaScript parser written in Go, compiled to WebAssembly",
  "type": "module",
  "main": "./dist/node.cjs",
  "module": "./dist/node.js",
  "browser": "./dist/browser.js",
  "types": "./dist/node.d.ts",
  "exports": {
    ".": {
      "browser": {
        "types": "./dist/browser.d.ts",
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs"
      },
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js",
      "require": "./dist/node.cjs",
      "default": "./dist/browser.js"
    },
    "./go-fast.wasm": "./dist/go-fast.wasm",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:wasm": "cd .. && GOOS=js GOARCH=wasm go build -o npm/dist/go-fast.wasm ./wasm",
    "build:js": "tsup",
    "build": "npm run build:wasm && npm run build:js",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jxekav/go-fast.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "parser",
    "ast",
    "estree",
    "wasm",
    "webassembly",
    "go"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jxekav/go-fast/issues"
  },
  "homepage": "https://github.com/jxekav/go-fast#readme",
  "devDependencies": {
    "@types/node": "^24.10.2",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}